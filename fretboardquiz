server {
    listen 80;
    listen [::]:80;

    root /var/www/fretboard-quiz;
    index index.html;

    # server_name csasguitar.com www.csasguitar.com;
    server_name _;

    location / {
        try_files $uri $uri/ =404;
    }

    location /scores {
        include proxy_params;
        proxy_pass http://0.0.0.0:5000;
    }
}
